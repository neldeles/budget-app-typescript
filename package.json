{
  "name": "budget-app-v3",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/forms": "^0.3.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/mdx": "^2.0.1",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "axios": "^0.24.0",
    "faker": "^5.5.3",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-datetime": "^3.1.1",
    "react-dom": "^17.0.2",
    "react-query": "^3.33.1",
    "react-router-dom": "6",
    "react-scripts": "4.0.3",
    "react-table": "^7.7.0",
    "typescript": "^4.4.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "TAILWIND_MODE=watch craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "TAILWIND_MODE=watch start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prettier": "prettier .",
    "format:check": "yarn prettier --check",
    "format:fix": "yarn prettier --write",
    "type:check": "tsc",
    "prepare": "husky install",
    "lint": "eslint --ext .tsx,.ts ."
  },
  "eslintConfig": {
    "plugins": [
      "tailwindcss"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:tailwindcss/recommended",
      "plugin:storybook/recommended",
      "plugin:mdx/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ],
    "settings": {
      "tailwindcss": {
        "callees": [
          "classnames",
          "clsx",
          "ctl",
          "classNames"
        ]
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mswjs/data": "^0.8.0",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-interactions": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/node-logger": "^6.4.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.7",
    "@storybook/testing-library": "^0.0.7",
    "@types/react-table": "^7.7.8",
    "autoprefixer": "^9",
    "chromatic": "^6.0.6",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-storybook": "^0.5.3",
    "eslint-plugin-tailwindcss": "^1.17.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "msw": "^0.35.0",
    "msw-storybook-addon": "^1.5.0",
    "postcss": "^7",
    "prettier": "^2.4.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "wallaby": {
    "autoDetect": true,
    "maxLogEntrySize": 40000
  }
}
